border-radius: 8px;
border: 1px solid gray;
padding: 0 8px;
background-color: #FFFFFF;
}

QComboBox::drop-down {
     subcontrol-origin: padding;
     subcontrol-position: top right;
     width: 15px;

     border-left-width: 1px;
     border-left-color: darkgray;
     border-left-style: solid; /* только одна линия */
     border-top-right-radius: 3px; /* тот же радиус закругления что и у QComboBox */
     border-bottom-right-radius: 3px;
 }

QComboBox::down-arrow {
     image: url(./resources/down-arrow.png);
 }

QComboBox::down-arrow:on { /* сдвиг стрелки, когда раскрывается всплывающий список */
     top: 1px;
     left: 1px;
 }

 QComboBox:editable {
     background: white;
 }

 QComboBox:!editable, QComboBox::drop-down:editable {
      background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 #E1E1E1, stop: 0.4 #FFFFFF,
                                  stop: 0.5 #FFFFFF, stop: 1.0 #D3D3D3);
 }

 /* QComboBox получает состояние &quot;on&quot;, когда всплывающий список раскрыт */
 QComboBox:!editable:on, QComboBox::drop-down:editable:on {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #D3D3D3, stop: 0.4 #FFFFFF,
                                 stop: 0.5 #FFFFFF, stop: 1.0 #E1E1E1);
 }</string>
